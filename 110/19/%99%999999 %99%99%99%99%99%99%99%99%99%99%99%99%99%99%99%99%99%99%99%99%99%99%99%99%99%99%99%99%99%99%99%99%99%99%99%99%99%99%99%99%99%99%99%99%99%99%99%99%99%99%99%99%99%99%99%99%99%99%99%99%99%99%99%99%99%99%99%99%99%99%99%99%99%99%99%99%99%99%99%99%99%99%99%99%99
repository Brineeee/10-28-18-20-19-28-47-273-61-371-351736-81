local Luxtl = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Luxware-UI-Library/main/Source.lua"))()

local Luxt = Luxtl.CreateWindow("SkeleHub", 0) 

local mainTab = Luxt:Tab("Farms", "rbxthumb://type=Asset&id=5107182114&w=150&h=150")
local eggTab = Luxt:Tab("Eggs", "rbxthumb://type=Asset&id=5107182114&w=150&h=150" )
local miscTab = Luxt:Tab("Miscs", "rbxthumb://type=Asset&id=5107182114&w=150&h=150")

local new77 = mainTab:Section("Farms | Auto Farm")
new77:Button("How to use : Rename pet to 1,2,3,4 ", function() loadstring(game:HttpGet("https://raw.githubusercontent.com/Brineeee/autofarm/main/lua-to-obfuscate"))() 
end)

local new777 = eggTab:Section("Eggs | Normal") 

new777:Button("Stop Hatch", function()
local eggName="Air Egg" -- Set to the egg you want to hatch
local TripleHatch=false -- Set to true if you want triple hatch (gamepass needed)
_G.Stop=true -- Set to true to stop the script.


local eggRemote=game: GetService ("Workspace")["__THINGS"]["__REMOTES"]["buy egg"]

while wait () do
    eggRemote:InvokeServer({eggName, TripleHatch})
    if _G.Stop then
        break
    end
end
end) 

